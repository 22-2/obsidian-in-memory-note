{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"noEmit": true,
		"baseUrl": ".",
		"verbatimModuleSyntax": true,
		"downlevelIteration": true,
		"resolveJsonModule": true,
		"lib": ["DOM", "ESNext"],
		"types": ["node", "obsidian-typings", "vitest/globals"]
	},
	"include": ["src/**/*", "src/*", "**/*.ts"],
	"exclude": ["node_modules/*"]
}
